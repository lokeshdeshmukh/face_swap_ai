ENV=dev
DATABASE_URL=sqlite:///./truefaceswap.db

# local | s3
STORAGE_BACKEND=s3
DATA_ROOT=../data

AWS_PROFILE=schoollm
S3_REGION=us-east-1
S3_BUCKET=truefaceswapvideo-schoollm-738149121200
S3_PREFIX=truefaceswapvideo

# Only needed for local-storage asset URLs and callback endpoint routing.
# If you keep callback-based completion, this must be a public URL.
PUBLIC_BASE_URL=https://your-tunnel-domain.example.com

RUNPOD_ENABLED=true
RUNPOD_API_KEY=your-runpod-api-key
RUNPOD_ENDPOINT_ID=your-endpoint-id
RUNPOD_API_BASE=https://api.runpod.ai/v2

ASSET_TOKEN_SECRET=replace-with-random-secret
ASSET_TOKEN_TTL_SECONDS=900
OUTPUT_URL_TTL_SECONDS=3600
CALLBACK_SECRET=replace-with-random-callback-secret

MAX_UPLOAD_MB=500
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
